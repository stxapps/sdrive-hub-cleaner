{
  "name": "sdrive-hub-cleaner",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "20.x.x"
  },
  "type": "module",
  "scripts": {
    "start": "node --import=specifier-resolution-node/register src/index.js",
    "cleanDeletedSDriveHub": "node --import=specifier-resolution-node/register src/cleanDeletedSDriveHub.js --dry-run",
    "cleanSDriveHub": "node --import=specifier-resolution-node/register src/cleanSDriveHub.js --dry-run",
    "cleanFileLog": "node --import=specifier-resolution-node/register src/cleanFileLog.js --dry-run",
    "cleanFileWorkLog": "node --import=specifier-resolution-node/register src/cleanFileWorkLog.js --dry-run",
    "cleanDeletedSDriveHub-local": "GOOGLE_APPLICATION_CREDENTIALS=~/.config/gcloud/legacy_credentials/brace@brace.to/sdrive-001-adeb2f2dcdc2.json node --import=specifier-resolution-node/register src/cleanDeletedSDriveHub.js --dry-run",
    "cleanSDriveHub-local": "GOOGLE_APPLICATION_CREDENTIALS=~/.config/gcloud/legacy_credentials/brace@brace.to/sdrive-001-adeb2f2dcdc2.json node --import=specifier-resolution-node/register src/cleanSDriveHub.js --dry-run",
    "cleanFileLog-local": "GOOGLE_APPLICATION_CREDENTIALS=~/.config/gcloud/legacy_credentials/brace@brace.to/sdrive-001-adeb2f2dcdc2.json node --import=specifier-resolution-node/register src/cleanFileLog.js --dry-run",
    "cleanFileWorkLog-local": "GOOGLE_APPLICATION_CREDENTIALS=~/.config/gcloud/legacy_credentials/brace@brace.to/sdrive-001-adeb2f2dcdc2.json node --import=specifier-resolution-node/register src/cleanFileWorkLog.js --dry-run",
    "deploy": "gcloud run jobs deploy sdrive-hub-cleaner --source . --cpu=1 --memory=256Mi --tasks=1 --max-retries=0 --task-timeout=20m --region=us-central1 --project=sdrive-001"
  },
  "dependencies": {
    "@google-cloud/datastore": "^9.2.1",
    "@google-cloud/storage": "^7.14.0",
    "specifier-resolution-node": "^1.1.4"
  }
}
